<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"></setting>
	</settings>

	<typeAliases>
		<typeAlias type="kr.co.moviejd.member.domain.Member" alias="Member"/>
		<typeAlias type="kr.co.moviejd.notice.domain.Notice" alias="Notice"/>
		<typeAlias type="kr.co.moviejd.notice.domain.Reply" alias="Reply"/>
	</typeAliases>
<!-- 	매퍼 만들고 등록하기 -->
	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
